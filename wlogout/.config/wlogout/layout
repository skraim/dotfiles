{
    "label" : "lock",
    "action" : "hyprlock",
    "text" : "[ X ]",
    "keybind" : "x"
}
{
    "label" : "logout",
    "action" : "hyprctl dispatch exit",
    "text" : "[ E ]",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "~/scripts/fastfetch-screen.sh; systemctl poweroff",
    "text" : "[ S ]",
    "keybind" : "s"
}
{
    "label" : "suspend",
    "action" : "systemctl suspend",
    "text" : "[ U ]",
    "keybind" : "u"
}
{
    "label" : "reboot",
    "action" : "~/scripts/fastfetch-screen.sh; systemctl reboot",
    "text" : "[ R ]",
    "keybind" : "r"
}
